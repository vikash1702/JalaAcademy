import java.io.*;
import java.lang.Math;

class Main {
    public static boolean isPrime(int a){
        boolean flag = true;
        for(int i=2;i<=Math.sqrt(a);i++){
            if(a%i==0){
                flag = false;
                break;
            }
        }
        return flag;
    }
	public static void main (String[] args) {
	    if(isPrime(23))
	    System.out.println("It is prime number");
	    else
	    System.out.println("It is not prime number");
	}
}
