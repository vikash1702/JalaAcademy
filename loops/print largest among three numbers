import java.io.*;

class Main {
    public static int largest(int a, int b, int c){
        if(a>b){
            if(a>c) return a;
            else return c;
        }else {
            if(b>c) return b;
            else return c;
        }
    }
	public static void main (String[] args) {
	 
	 System.out.println(largest(10,13,15));
	}
}
