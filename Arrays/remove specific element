import java.io.*;


class Main {
    public static void removeElement(int[] array,int number){
       
       for(int i=0;i<array.size;i++){
           if(number==array[i]){
               array[i] = 0;
           }
       }
       
    }
	public static void main (String[] args) {
	    int[] array = new int[10];
	    
	    removeElement(array,10);
	}
}
