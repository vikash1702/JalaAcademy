
import java.io.*;


class Main {
    public static int average(int[] array,int size){
       int sum=0;
       for(int i=0;i<size;i++){
           sum = sum + array[i];
       }
       return sum/size;
    }
	public static void main (String[] args) {
	    int[] array = new int[10];
	    
	    System.out.println(average(array,10);)
	}
}
